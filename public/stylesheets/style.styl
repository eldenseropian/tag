body
  padding 3em
  font 14px "Lucida Grande", Helvetica, Arial, sans-serif
table
  margin-left auto
  margin-right auto
.noshow
  display none
#about
  position absolute
  width 21em
#help
  border 2px dotted orange
  border-radius 1em
  font-size .5em
  margin-left .5em
  padding-left .4em
  padding-right .4em
#picture-choices
  td
    padding .25em
#previous-link:hover
  color orange
#previous-link
  color white
  font-size 2em
  margin-left 13em
  position absolute
  text-shadow -1px 0 black, 0 1px black, 1px 0 black, 0 -1px black
#status
  margin-top 2.5em
  margin-bottom 2.5em
  text-align center
  td
    padding-left 3.25em
    padding-right 3.25em
#overlay
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.6;
  z-index: 5;
#popup
  position:absolute;
  text-align: center;
  top:200px;
  left:50%;
  width:400px;
  height:566;
  margin-left:-250px;
  border:7px solid orange;
  background-color:white;
  z-index: 10;
#refresh:hover
  color orange
.tags
  border 1px dotted gray
  height 29em
  li:hover
    color orange
  margin 1em
  margin-bottom 0em
  overflow scroll
  width 10em
  ul
    list-style-type none
    li
      margin-top 1em